		{
			case "A":
				break
			case "B":
				break			
			default:
		}
	5. (10>5)? "10 is greater than 5" : "5 is greater than 10"
	
***************** Day 7: Loops ***************** 
	1. for(let i=1; i<10; i++) (Tredetional Loop)
		{
		}
